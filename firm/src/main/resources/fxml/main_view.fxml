<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="700.0" minWidth="1140.0" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="polytech.unice.si3.ihm.firm.controller.MainViewController">
   <children>
      <VBox prefHeight="720.0" prefWidth="1280.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane>
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <ImageView fx:id="logo" fitHeight="120.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <image>
                              <Image url="@/images/Logoenseigne.png" />
                           </image>
                        </ImageView>
                        <VBox alignment="CENTER" prefHeight="167.0" prefWidth="1148.0">
                           <children>
                              <ImageView fx:id="firmImageName" fitHeight="150.0" fitWidth="646.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@/images/baniere.png" />
                                 </image>
                              </ImageView>
                              <Label fx:id="description" text="&quot;Description possible en label aussi&quot;" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets left="6.0" right="6.0" top="6.0" />
               </padding>
            </AnchorPane>
            <AnchorPane prefHeight="541.0" prefWidth="1280.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="1280.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <VBox alignment="TOP_CENTER">
                           <children>
                              <ComboBox fx:id="searchType" prefHeight="29.0" prefWidth="180.0" promptText="Sélectionner le type" />
                              <HBox prefHeight="104.0" prefWidth="200.0">
                                 <children>
                                    <TextField fx:id="searchValue" promptText="Tapez une ville, une région, une adresse ..." />
                                    <Button fx:id="searchButton" mnemonicParsing="false" onMouseClicked="#search" onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" prefHeight="26.0" prefWidth="57.0" text="Rechercher" />
                                 </children>
                              </HBox>
                              <AnchorPane>
                                 <children>
                                    <VBox prefHeight="200.0" prefWidth="169.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                       <children>
                                          <Label fx:id="mallName" text="Nom du centre commercial où se trouve l'enseigne" />
                                          <Label fx:id="shopAdress" text="Adresse du centre commercial" />
                                          <Hyperlink fx:id="shopLink" text="Site internet du centre commercial" />
                                       </children>
                                    </VBox>
                                 </children>
                              </AnchorPane>
                              <Button fx:id="shopsButton" mnemonicParsing="false" onMouseClicked="#openAllShops" onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" text="Voir tous les magasins" />
                           </children>
                        </VBox>
                        <VBox prefHeight="456.0" prefWidth="925.0">
                           <children>
                              <VBox alignment="TOP_RIGHT" prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <ComboBox prefWidth="150.0" promptText="typeDeCarousel">
                                       <VBox.margin>
                                          <Insets right="5.0" />
                                       </VBox.margin>
                                    </ComboBox>
                                    <HBox alignment="CENTER" prefHeight="494.0" prefWidth="924.0">
                                       <children>
                                          <ImageView fx:id="carrouseImg1" fitHeight="350.0" fitWidth="220.0" onMouseClicked="#choseLeftProduct" onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@/images/produit_phare_3.jpg" />
                                             </image>
                                             <HBox.margin>
                                                <Insets right="16.0" />
                                             </HBox.margin>
                                          </ImageView>
                                          <ImageView fx:id="carrouseImg2" fitHeight="430.0" fitWidth="220.0" onMouseClicked="#choseCenterProduct" onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" pickOnBounds="true" preserveRatio="true">
                                             <HBox.margin>
                                                <Insets left="16.0" right="16.0" />
                                             </HBox.margin>
                                             <image>
                                                <Image url="@/images/produit_phare_1.png" />
                                             </image>
                                          </ImageView>
                                          <ImageView fx:id="carrouseImg3" fitHeight="350.0" fitWidth="220.0" onMouseClicked="#choseRightProduct" onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" pickOnBounds="true" preserveRatio="true">
                                             <HBox.margin>
                                                <Insets left="16.0" />
                                             </HBox.margin>
                                             <image>
                                                <Image url="@/images/produit_phare_2.png" />
                                             </image>
                                          </ImageView>
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <VBox prefHeight="488.0" prefWidth="195.0">
                           <children>
                              <Label fx:id="promo1" text="Info/Promo de la semaine 1" />
                              <Label fx:id="promo2" text="Info/Promo de la semaine 2" />
                              <Label fx:id="promo3" text="Info/Promo de la semaine 3" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets left="6.0" right="6.0" top="6.0" />
               </padding>
            </AnchorPane>
            <AnchorPane>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </AnchorPane>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="1280.0">
               <children>
                  <Hyperlink fx:id="firmLink" onMousePressed="#linkPressed" text="Plus d'informations" />
               </children>
               <padding>
                  <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
